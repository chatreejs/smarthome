{
  "name": "smarthome",
  "description": "This is an app that will help you track everything in your house such as electricity, water, food, etc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/chatreejs/smarthome.git"
  },
  "version": "0.1.0",
  "authors": [
    {
      "name": "Chanon Treemeth",
      "email": "chanon.t@chatree.dev"
    }
  ],
  "private": false,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "vite",
    "start:develop": "env-cmd -f .env.develop vite",
    "build": "tsc && vite build",
    "build:develop": "tsc && env-cmd -f .env.develop vite build",
    "build:prod": "tsc && env-cmd -f .env.prod vite build",
    "preview": "vite preview",
    "check-types": "tsc --noemit",
    "eslint": "eslint",
    "lint": "npm run eslint; npm run check-types",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@fontsource/sarabun": "^5.0.3",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@stomp/stompjs": "^7.0.0",
    "antd": "5.12.5",
    "axios": "^1.4.0",
    "dayjs": "^1.11.10",
    "husky": "^9.1.6",
    "rc-picker": "^3.14.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-oauth2-code-pkce": "^1.20.1",
    "react-router-dom": "^6.10.0",
    "rxjs": "^7.8.1",
    "styled-components": "^5.3.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/compat": "^1.0.1",
    "@eslint/js": "^9.2.0",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^4.1.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "globals": "^15.9.0",
    "env-cmd": "^10.1.0",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.9.0",
    "vite": "^4.5.0",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-pwa": "^0.17.4",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
